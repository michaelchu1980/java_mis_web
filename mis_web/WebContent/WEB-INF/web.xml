<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>mis_web</display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CheckAccount</servlet-name>
    <servlet-class>com.mis.servlet.CheckAccount</servlet-class>
  </servlet>
  <servlet>
    <display-name>CheckLogin Servlet</display-name>
    <servlet-name>user_login</servlet-name>
    <servlet-class>com.mis.servlet.user_login</servlet-class>
  </servlet>
  
    <servlet>
    <display-name>Cadlog Servlet</display-name>
    <servlet-name>cadlog</servlet-name>
    <servlet-class>com.mis.servlet.CadLogDataBLL</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
    <servlet-name>CheckAccount</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
    <servlet-mapping>
    <servlet-name>cadlog</servlet-name>
    <url-pattern>/cadlog</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>user_login</servlet-name>
    <url-pattern>/checkLogin</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
</web-app>